{
  "manifest_version": 3,
  "name": "Keep Alive",
  "version": "1.0",
  "description": "Keep your session alive by sending periodic requests.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
